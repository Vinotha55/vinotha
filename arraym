#include<stdio.h>
int main()
{
int a[2][2],b[2][2],c[2][2]={0},i,j,z;
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
        printf("Enter the number");
        scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
    {
        printf("Enter the number");
        scanf("%d",&b[i][j]);
    }
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            for(z=0;z<2;z++)
            
            c[i][j]+=a[i][z]*b[z][j];
        }
    }
    printf("The output is :");
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
        {
            printf("\nNumber of c[%d][%d] is %d",i,j,c[i][j]);
        }
    }
    return 0;
}





